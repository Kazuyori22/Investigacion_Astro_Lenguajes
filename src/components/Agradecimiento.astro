---
const { mensaje, content } = Astro.props;
---

<div style="font-family: Arial, sans-serif; background-color: #8181dd4f; color: white; padding: 50px 0; text-align: center;">
  <h2>{mensaje}</h2>
  <p>{content}</p>
  <a href="/" style="text-decoration: none; background-color: #e67e22; padding: 10px 20px; border-radius: 5px; color: white; margin-top: 20px; display: inline-block;">
    Regresar a Inicio
  </a>
</div>
